<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item >
        <shape android:shape="rectangle">
            <solid 
                android:color="#f7f7f7"
                />
            <corners 
                android:radius="5dp"
                />
            <size 
                android:width="20dp"
                android:height="20dp"
                />
